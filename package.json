  "scripts": {
    "start-search": "cp src/search/index.js src/index.js && react-scripts start",
    "start-dbform": "cp src/dbform/index.js src/index.js && react-scripts start",
    "start-predictor": "cp src/predictor/index.js src/index.js && react-scripts start",
    "start-ir-parse": "cp src/ir-parse/index.js src/index.js && react-scripts start",
    "tests": "jest",
    "start-api-server": "json-server --watch db.json --port 3001",
    "build": "react-scripts build && npm run build:clean",
